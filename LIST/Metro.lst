Nstatic __inline int SoftwarePLL(int Ua, int Ub, int Uc, int Theta) {
Nint Va;
Nint Vb;
N	
Nint Vq;	
N
N	/* Clark transform 
N	| Va |               |    1      cos(2*pi/3)	cos(4*pi/3) | |Ua|               | sin(wt) |
N	| Vb | = sqrt(2/3) * |    0      sin(2*pi/3)	sin(4*pi/3) |*|Ub| = sqrt(2/3) * | sin(wt) | * V
N	| Vo |               | 1/sqrt(2)  1/sqrt(2)    1/sqrt(2)  | |Uc|               |    0    |
N
N	cos(2*pi/3) = -0.5
N	cos(4*pi/3) = -0.5
N
N	sin(2*pi/3) =  sqrt(3)/2 
N	sin(4*pi/3) = -sqrt(3)/2	
N	*/
N
N  Va = ((Ua - (Ub >> 1) - (Uc >> 1)) * 0xD106ull) >> 16 ;
N	Vb = ((Ub - Uc) *0xB505ull ) >> 16;
N	
N	/* Park transform	
N
N	Vq = -sin(theta)*Va + cos(theta)*Vb
N
N	*/
N
N	Theta += PrgData.IncPhaseCalc;
X	Theta += Metro .IncPhaseCalc;
N  Theta	&= 0xFFFF;
N	
N	Vq = -sinf(Pi * Theta / 0x8000) * Va + cosf(Pi * Theta / 0x8000) * Vb;
X	Vq = -sinf(3.1415926535897932384626433832795f * Theta / 0x8000) * Va + cosf(3.1415926535897932384626433832795f * Theta / 0x8000) * Vb;
N
N	PrgData.IncPhaseCalc = PI_REG(__SSAT(Vq,16), 0, &PrgData.BufFiltdPhase, &SavePrms.RegPLL);
X	Metro .IncPhaseCalc = PI_REG(__ssat(Vq,16), 0, &Metro .BufFiltdPhase, &Metro . SavedParam . SavedParam .RegPLL);
N
N	return Theta;//(Theta + PrgData.IncPhaseCalc) & 0xFFFF;
N}
