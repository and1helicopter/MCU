Nint  ModbusErr(int ErrCode, unsigned char *RxBuff, unsigned char *TxBuff) {
Nunsigned short CRCCode;
N	((short *)TxBuff)[0] = ((short *)RxBuff)[0] | 0x8000;
N	TxBuff[2] = ErrCode;
N	CRCCode   = CalcCRC(3, TxBuff);
N	TxBuff[3] = CRCCode;
N	TxBuff[4] = CRCCode >> 8;
N	return 5;
N}	
