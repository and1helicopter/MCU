Nunsigned short CalcCRC(int Count, unsigned char *Buff) {
N  unsigned short CRCCode = 0xFFFF;
N  int i;
N	for (i = 0; i < Count; i++) CRCCode = ((CRCCode >> 8) & 0xFF) ^ CRCTable[(CRCCode ^ Buff[i]) & 0xFF];
N	return CRCCode;
N}
